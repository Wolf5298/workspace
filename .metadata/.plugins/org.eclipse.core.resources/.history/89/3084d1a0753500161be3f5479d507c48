/**
 * The test class for the CandleImpl class implementation of the interface
 * Candle.
 * 
 * @author Chris
 *
 */
public class CandleImplTest {

	// Fields of Type CandleImpl
	private CandleImpl _candle1;
	private CandleImpl _candle2;
	private CandleImpl _candle3;
	private CandleImpl _candle4;

	//initialisates the Fields to be tested.
	public CandleImplTest() {
		
		_candle1 = new CandleImpl(10.8);
		
		_candle2 = new CandleImpl(10.8);
		_candle2.refresh(11.5);
		
		_candle3 = new CandleImpl(10.8);
		_candle3.refresh(11.5);
		_candle3.refresh(8.7);
		
		_candle4 = new CandleImpl(10.8, 11.5, 8.7, 11.3);
		
		private CandleImpl test = new CandleImpl();
	}
}
